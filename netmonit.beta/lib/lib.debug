# ON / OFF
DEBUG_1="OFF"
DEBUG_2="OFF"


DEBUG_1()
{
    if [[ "$DEBUG_1" == "ON" ]] ; then
        echo -e "	${YW} DEBUG: $1 ${NC}"
    fi
}

DEBUG_2()
{
    if [[ "$DEBUG_2" == "ON" ]] ; then
        echo "$1"
    fi
}

